{
  "mod": {
    "prefix": [
      "mod"
    ],
    "description": "Module",
    "body": [
      "local M = {}",
      "${0}",
      "return M"
    ]
  },
  "rpaths": {
    "prefix": [
      "rpaths"
    ],
    "description": "require paths",
    "body": [
      "local plug = require('jg.paths')"
    ]
  },
  "require": {
    "prefix": [
      "require"
    ],
    "body": [
      "local $1 = require('${2:$1}')"
    ]
  },
  "dump": {
    "prefix": [
      "dump"
    ],
    "body": [
      "print(vim.inspect($1))"
    ]
  },
  "layer import": {
    "prefix": "layer:i",
    "body": [
      "layer.use({",
      "  requires = { '$1' },",
      "",
      "  after = function()",
      "    $2",
      "  end,",
      "})"
    ]
  },
  "layer": {
    "prefix": "layer",
    "body": [
      "layer.use({",
      "  requires = {",
      "    '${1:$CLIPBOARD}',",
      "  },$0",
      "})"
    ]
  },
  "layer setup": {
    "prefix": "setup",
    "body": [
      "setup = function()",
      "  $0",
      "end,"
    ]
  },
  "layer enabled": {
    "prefix": "enabled",
    "body": [
      "enabled = ${1|true,false|},$0"
    ]
  },
  "autocmd": {
    "prefix": [
      "autocmd"
    ],
    "body": [
      "vim.api.nvim_create_autocmd('${1:$CLIPBOARD}', {",
      "  callback = function()",
      "    $0",
      "  end,",
      "})"
    ]
  },
  "desc": {
    "prefix": [
      "desc"
    ],
    "body": [
      "describe('gx', function ()",
      "\t$1",
      "end)"
    ]
  },
  "its": {
    "prefix": [
      "its"
    ],
    "body": [
      "it('should $1', function()",
      "\t$2",
      "end)"
    ]
  }
}
